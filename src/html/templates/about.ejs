<%- include('../partials/head'); %>
    <%- include('../partials/header'); %>
    <%- include('../partials/nav'); %>

<main>
    <section class="main__section">
        <h1 class="main__section-h1">About me</h1>
        <p>Hey my name is Mickey Fischer Hansen
            im 18 years old and i live in Denmark, i am studying webdevelopment at Roskilde Tekniske Skole.</p>
    </section>

    <section class="form__section">
        <h1>Contact</h1>
        <div id="error"></div>
    <form id="form" name="contact" method="POST" data-netlify="true">

    <div>
        <label for="name">Name</label>
        <input id="name" name="name" type="text">
    </div>

    <div>
        <label for="subject">Subject</label>
        <input id="subject" name="subject" type="text">
    </div>

    <div>
        <label for="phone">Phone</label>
        <input id="phone" name="phone" type="text">
    </div>

    <div>
        <label for="email">Email</label>
        <input id="email" name="email" type="text">
    </div>

    <div class="field">
        <textarea name="message" id="message" 
        placeholder="Message"></textarea>
    </div>

    <div class="field">
    <input type="file" name="myfile"
    id="myfile" placeholder="Upload"
    rows="7">
    </div>
    <div>

    </div class="field">
        <div data-netlify-recaptcha="true">
            
        </div>
    <div>
        <button class="btn">
            <input type="submit" id="submit"> 
        </button>
    </div>

</form>
</section>
   
</main>

<%- include('../partials/footer'); %>
<%- include('../partials/after-footer'); %>